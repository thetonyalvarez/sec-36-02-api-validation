{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "author",
        "language",
        "pages",
        "title",
        "year"
    ],
    "properties": {
        "author": {
            "type": "string",
            "default": "",
            "title": "The author Schema",
            "examples": [
                "Matthew Lane"
            ]
        },
        "language": {
            "type": "string",
            "default": "",
            "title": "The language Schema",
            "examples": [
                "english"
            ]
        },
        "pages": {
            "type": "integer",
            "default": 0,
            "title": "The pages Schema",
            "examples": [
                264
            ]
        },
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Power-Up: Unlocking the Hidden Mathematics in Video Games"
            ]
        },
        "year": {
            "type": "integer",
            "default": 0,
            "title": "The year Schema",
            "examples": [
                2017
            ]
        }
    },
    "examples": [{
        "author": "Matthew Lane",
        "language": "english",
        "pages": 264,
        "title": "Power-Up: Unlocking the Hidden Mathematics in Video Games",
        "year": 2017
    }]
}